{
    "name" : "视频搜索",
    "version": "0.0.0.5",
    "description": "豌豆荚视频搜索",
    "icons": {
        "16": "icon16.png",
        "72": "icon72.png"
    },
    "privacy_permissions": [
        "device", "window", "apps", "video"
    ],
    "permissions" : [
        "file:///*/*"
    ],
    "content_scripts": [
        {
            "matches": [""]
        }
    ],
    "minimum_client_version" : "2.49",
    "app": {
        "launch": {
            "web_url": "wdj-extension://__MSG_@@extension_id__/index.html"
        },
        "navigation": [{
            "label": "首页",
            "web_url": "wdj-extension://__MSG_@@extension_id__/index.html"
        }, {
            "label": "电视剧",
            "web_url": "wdj-extension://__MSG_@@extension_id__/cate.html#tv"
        }, {
            "label": "电影",
            "web_url": "wdj-extension://__MSG_@@extension_id__/cate.html#movie"
        }, {
            "label": "动漫",
            "web_url": "wdj-extension://__MSG_@@extension_id__/cate.html#comic"
        }, {
            "label": "综艺",
            "web_url": "wdj-extension://__MSG_@@extension_id__/cate.html#variety"
        }]
    }
}
